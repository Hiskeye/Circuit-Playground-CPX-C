name = input("What is your name: ")
language = input("Pick a language: English, Spanish, French, Swahili, Ukrainian, or Middle Earth Elven: ")

def greet(lang):
    if lang == 'spanish':
        return'Hola'
    elif lang == 'french':
        return'Bonjour'
    elif lang == 'swahili':
        return 'habari'
    elif lang == 'ukrainian':
        return 'привіт'
    elif lang == 'middle earth elven':
        return 'Ai'
    else:
        return'Hello'

def main():
    print(greet(language),name)
    
main()
